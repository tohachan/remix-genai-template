{
  "name": "remix-genai-template",
  "version": "0.5.0",
  "private": true,
  "author": "Anton An",
  "scripts": {
    "dev": "remix vite:dev",
    "dev:api": "json-server --port 3001 db.json",
    "dev:full": "concurrently \"npm run dev:api\" \"npm run dev\"",
    "build": "remix vite:build",
    "start": "remix-serve ./build/server/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "generate:readme": "node tools/generator/readme.js",
    "generate:component": "node tools/generator/component.js",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "@remix-run/node": "^2.16.8",
    "@remix-run/react": "^2.16.8",
    "@remix-run/serve": "^2.16.8",
    "clsx": "^2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.2.0",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@remix-run/dev": "^2.16.8",
    "@shadcn/ui": "latest",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.10",
    "@types/jest": "^29.5.12",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "autoprefixer": "^10.4.13",
    "concurrently": "^9.1.2",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^10.1.0",
    "handlebars": "^4.7.8",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^8.2.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "jsdom": "^26.1.0",
    "json-server": "^1.0.0-beta.3",
    "plop": "^4.0.1",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.1.5",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  }
}
